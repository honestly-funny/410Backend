{
	"name": "cs410-chrome-extension",
	"version": "0.1",
	"description": "Description of extension",
	"manifest_version": 2,
	"permissions": ["activeTab", "storage", "tabs"],
	"browser_action": {         
      "default_popup": "popup.html"
    },
	"content_scripts": [
		{
			"matches": ["https://www.reddit.com/*"],
			"js": ["main.js"]
		}
	]

}